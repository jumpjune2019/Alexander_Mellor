import java.lang.Math;
public class Main {
	private static double getRandomNum() {
		return Math.floor(Math.random() * 2) + 1;
	}
	public static void main(String[] args) {
		int tally[] = new int[2];
		int heads, tails;
		System.out.println("1000 Coin Flips.");
