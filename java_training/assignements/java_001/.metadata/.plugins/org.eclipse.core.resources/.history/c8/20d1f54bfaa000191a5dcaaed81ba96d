
public class Main {
	public static void main(String[] args) {
		byte loopControl = 7;
		String diceRoll[][] = {
				{"", "One", "Two", "Three", "Four", "Five", "Six"},
				{"One", "Snake Eyes", "Astrailian Yo", "Little Jo From Kokomo", "No Field Five", "Easy Six", "Six One You're Done"},
				{"Two", "Ace Caught a Duece", "Ballarena", "The Fever", "Jimmie Hicks", "Benny Blue", "Easy Eight"},
				{"Three", "Easy Four", "OJ", "Brooklen Forest", "Big Red", "Eighter From Decatur", "Nina From Pasadena"},
				{"F", "Little Phoebe", "Easy Six", "Skinny McKinney", "Square Pair", "Railroad Nine", "Big One On The End"},
				{"5", "Sixie From Dixie", "Skinny Dugan", "Easy Eight", "Jessie James", "Puppy Paws", "Yo"},
				{"6", "The Devil", "Easy Eight", "Lou Brown", "Tennessee", "Six Five No Jive", "Midnight"}
		};
		for(byte x = 0; x < loopControl; x++) {
			for(byte y = 0; y < loopControl; y++) {
				System.out.print(diceRoll[x][y]);
				System.out.print("\t");
			}
			System.out.print("\n");
		}
	}
}
