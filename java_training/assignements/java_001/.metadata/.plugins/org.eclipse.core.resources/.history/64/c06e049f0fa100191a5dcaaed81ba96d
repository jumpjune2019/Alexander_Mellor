		int found[];
		if(caseSensitive == true) {
			
		}else if(caseSensitive == false) {
			
		}
		return found;
	}
	public static void main(String[] args) {
		String myList[] = {"Bozo", "FooBar", "Delta", "Foozball", "Demo", "Money", "Zoo"};
	}
}
